{
  "name": "eslint-rich-reporter",
  "version": "0.0.3",
  "description": "An eslint html reporter with several useful features.",
  "main": "reporter.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint -c ./configs/.eslintrc.js 'src/**/*.ts'",
    "build": "sh ./scripts/build.sh",
    "clear": "rm -rf build report-demo.html templates reporter.js",
    "pre-publish": "yarn build && cp -r build/* . && rm -rf build"
  },
  "keywords": [
    "eslint",
    "formatter",
    "html",
    "reporter",
    "useful"
  ],
  "author": "Deland Li",
  "license": "ISC",
  "files": [
    "templates", "reporter.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-import": "^1.6.6",
    "commander": "^5.1.0",
    "enzyme": "^3.9.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.9.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "rollup": "^2.26.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "mustache": "^4.2.0"
  }
}
